{
  "name": "Balas Chat AI",
  "version": "1.0.0",
  "description": "Unified WhatsApp AI Chat",
  "type": "module",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:safe": "./kill-port.sh 3000 && npm run dev",
    "kill:3000": "./kill-port.sh 3000",
    "kill:port": "./kill-port.sh",
    "db:init": "node utils/migration-scripts/init-database.js",
    "db:setup": "npm run db:init && npm run seed:ai && npm run seed:business",
    "db:clean": "node utils/clean-database.js",
    "db:remove-device": "node utils/remove-device.js",
    "db:migrate-status": "node utils/migration-scripts/update-device-status-enum.js",
    "seed:ai": "node utils/seed-ai-providers.js",
    "seed:business": "node src/seeders/master-seeder.js",
    "seed:all": "node src/seeders/master-seeder.js everything",
    "migrate:ai": "node utils/migration-scripts/run-ai-migrations.js",
    "migrate:lid": "node utils/migration-scripts/add-lid-support.js"
  },
  "keywords": [
    "whatsapp",
    "api",
    "baileys",
    "rest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.0",
    "mysql2": "^3.6.5",
    "node-cron": "^4.0.7",
    "openai": "^4.100.0",
    "pino": "^9.7.0",
    "sequelize": "^6.32.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1"
  }
}
